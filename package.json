{
  "name": "personal-crm",
  "version": "1.115.0",
  "private": true,
  "scripts": {
    "install-dependencies": "npm i",
    "dev": "next dev",
    "prebuild": "tsc --noEmit",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "signin:code-assistant": "aws sso login --profile nxsflow-staging",
    "sandbox": "npx ampx sandbox --profile impulso",
    "sandbox:debug": "npx ampx sandbox --profile impulso --debug",
    "import-data-dev": "cd scripts && node start-import -env dev",
    "import-data-staging": "cd scripts && node start-import -env staging",
    "import-data-prod": "cd scripts && node start-import -env prod",
    "create-code": "npx ampx generate graphql-client-code --out amplify/graphql-code --profile impulso"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
